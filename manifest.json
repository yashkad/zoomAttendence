{
    "manifest_version": 2,
    "name": "Zoom Attendence by Yash",
    "version": "0.1",
    "description": "Creates txt file for attendence",
    "author": "Yash Kadam",
    "permissions": [
        "tabs",
        "storage"
    ],

    "content_scripts": [{
        "matches": ["*://zoom.us/*", "*://us04web.zoom.us/*"],
        "js": ["contentScript.js"]
    }],


    "browser_action": {
        "default_icon": "Meet2.png",
        "default_popup": "popup.html",
        "default_title": "Zoom Attendance to txt file \n  \tBy Yash"
    },
    "icons": {
        "16": "Meet2.png",
        "48": "Meet2.png",
        "128": "Meet2.png"
    }
}